<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>DelicacyFusion-Web</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>Html2JSPServlet</display-name>
    <servlet-name>Html2JSPServlet</servlet-name>
    <servlet-class>rps.dev.web.Html2JSPServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Html2JSPServlet</servlet-name>
    <url-pattern>*.html</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Css2LessServlet</display-name>
    <servlet-name>Css2LessServlet</servlet-name>
    <servlet-class>rps.dev.web.Css2LessServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Css2LessServlet</servlet-name>
    <url-pattern>*.css</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LatencyServlet</display-name>
    <servlet-name>LatencyServlet</servlet-name>
    <servlet-class>rps.dev.web.LatencyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LatencyServlet</servlet-name>
    <url-pattern>/latency/*</url-pattern>
  </servlet-mapping>

  <servlet>
    <description></description>
    <display-name>MinifyJavaScriptServlet</display-name>
    <servlet-name>MinifyJavaScriptServlet</servlet-name>
    <servlet-class>rps.dev.web.MinifyJavaScriptServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MinifyJavaScriptServlet</servlet-name>
    <url-pattern>*.js</url-pattern>
  </servlet-mapping>

</web-app>