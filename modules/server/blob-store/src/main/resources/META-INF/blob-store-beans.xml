<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
	<bean id="imageService" class="com.df.blobstore.image.ImageService" autowire="default">
		<constructor-arg index="0" ref="imageServiceRoute" />
	</bean>
	<bean id="imageServiceRoute" class="com.df.blobstore.image.FileSystemImageServiceRoute" autowire="default">
		<constructor-arg index="0" ref="ImageKeyResolver" />
	</bean>
	<bean id="ImageKeyResolver" class="com.df.blobstore.image.DefaultImageKeyResolver" autowire="default"/>
</beans>